                  <Button fx:id="settingsButton" layoutX="20.0" layoutY="550.0" mnemonicParsing="false" onAction="#handleSettingsButton" text="Settings" />
                  <Button fx:id="copyButton" layoutX="20.0" layoutY="435.0" mnemonicParsing="false" text="copy" />
               </children>
            </AnchorPane>
        </items>
      </SplitPane>
   </children>
</VBox>
